{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "plugins": [
    "react",
    "react-native",
    "@typescript-eslint"
  ],
  "env": {
    "react-native/react-native": true,
    "es2021": true
  }
}